<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" type="text/css" href="~/css/style.css" />
</head>
<body>
    <div>
        <h1>Select City</h1>
        @foreach (var item in ViewBag.Cities)
        {
            @await Component.InvokeAsync("City", item.Key)
        }
    </div>
</body>
</html>
